{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": ["worker-configuration.d.ts"],
    "outDir": "./dist",
    "paths": {
      "$lib/*": ["./src/lib/*"],
      "$db/*": ["./src/db/*"],
      "$routers/*": ["./src/routers/*"]
    }
  },
  "exclude": ["node_modules"]
}
